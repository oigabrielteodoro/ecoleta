{
  "name": "@ecoleta/api",
  "version": "0.1.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node dist/src/index.js",
    "dev": "nodemon --exec sucrase-node src/index.ts",
    "build": "tsc",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "lint": "eslint . --ext js,ts",
    "lint:fix": "eslint --fix . --ext js,ts",
    "test": "jest --passWithNoTests && yarn test:integration",
    "test:watch": "jest --watch",
    "test:integration": "jest --passWithNoTests --config jest.config.integration.js",
    "test:integration:watch": " yarn test:integration --watch"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/module-alias": "2.0.1",
    "dotenv": "10.0.0",
    "module-alias": "2.2.2",
    "nodemon": "2.0.12",
    "sucrase": "3.20.1",
    "ts-jest": "27.0.5"
  },
  "dependencies": {
    "express": "4.17.1",
    "io-ts": "2.2.16",
    "io-ts-types": "0.5.16",
    "monocle-ts": "2.3.10",
    "newtype-ts": "0.3.4"
  }
}
